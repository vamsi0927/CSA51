#include <stdio.h>

char map(char c) {
    switch (c) {
        case '5': return 'a';
        case '3': return 'l';
        case 's': return 's';  // formerly 'â€¡'
        case 'h': return 'h';  // formerly 'â€ '
        case '0': return 'r';
        case ')': return 'i';
        case '6': return 'd';
        case '*': return 'm';
        case ';': return 'o';
        case '4': return 'n';
        case '8': return 'e';
        case '2': return 'y';
        case '.': return ' ';
        default: return '_'; // unknown
    }
}

int main() {
    char ct[] = "53ssh305))6*;4826)...";
    for (int i = 0; ct[i]; i++)
        printf("%c", map(ct[i]));
    printf("\n");
    return 0;
}
