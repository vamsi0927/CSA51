#include <stdio.h>
#include <ctype.h>

char decrypt(char c) {
    if (!isupper(c)) return c;
    int a = 11, b = 23, a_inv = 19;
    return ((a_inv * ((c - 'A' - b + 26)) % 26) + 'a');
}

int main() {
    char ct[] = "BUBBUUBBB";
    for (int i = 0; ct[i]; i++)
        printf("%c", decrypt(ct[i]));
    printf("\n");
    return 0;
}
